<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>PrintJspServlet_01</servlet-name>
        <servlet-class>jsp_01.PrintJspServlet_01</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentServlet</servlet-name>
        <servlet-class>jsp_01.StudentServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PrintJspServlet_01</servlet-name>
        <url-pattern>/jsp1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentServlet</servlet-name>
        <url-pattern>/jsp2</url-pattern>
    </servlet-mapping>
</web-app>